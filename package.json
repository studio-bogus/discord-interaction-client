{
  "name": "@studio-bogus/discord-interaction-client",
  "version": "1.0.0",
  "description": "Discord interaction rest client designed to work with cloudflare service workers.",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "release": "release-it"
  },
  "keywords": [],
  "author": "Gavenda <gavenda@disroot.org>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
